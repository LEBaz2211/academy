<a href="/user/add">Add a new user</a>
<h1>Users list</h1>

<ul>
	<% for (let i = 0 ; i < users.length ; i++)  { %>
	<li>
		<%= users[i].lastname %>  <%= users[i].firstname %>
		<a href="/user/connect/<%= i%>">Connect</a>
		<a href="/user/delete/<%= i%>">Delete</a>
		<a href="/user/show/:<%= i%>">Show</a>
	</li>
	<% } %>
</ul>